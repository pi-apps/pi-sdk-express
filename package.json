{
    "name": "pi-sdk-express",
    "version": "2.0.0",
    "description": "An Express.js SDK for Pi Network transaction processing",
    "keywords": [
        "pi",
        "pinetwork",
        "express",
        "sdk"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/pi-apps/pi-sdk-express"
    },
    "license": "SEE LICENSE IN LICENSE",
    "type": "module",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "vite build",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui"
    },
    "bin": {
        "pi-sdk-express-install": "./cli.js"
    },
    "peerDependencies": {
        "express": "^4.18.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^25.0.1",
        "express": "^4.18.2",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^1.4.0"
    },
    "dependencies": {
        "yarn": "^1.22.22"
    }
}
